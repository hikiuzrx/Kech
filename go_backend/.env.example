# Environment variables for Smart Waste Backend
# Copy this file to .env and fill in the values

# Server Configuration
SERVER_PORT=8080
SERVER_MODE=debug

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=smartwaste
DB_SSLMODE=disable

# MQTT Configuration
MQTT_BROKER=localhost
MQTT_PORT=1883
MQTT_CLIENT_ID=smartwaste-backend
MQTT_USERNAME=
MQTT_PASSWORD=

# Google Maps API (optional - for route optimization)
GOOGLE_MAPS_API_KEY=
